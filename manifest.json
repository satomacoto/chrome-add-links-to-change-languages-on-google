{
  "manifest_version": 2,
  "name": "Add links to change languages on Google\u2122",
  "version": "0.3",
  "default_locale": "en",
  "description": "",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": ["script.js"],
      "matches": ["<all_urls>"],
      "include_globs": ["*://*www.google*/search*"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "icon-128.png",
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png"
  },
  "page_action": {
    "default_icon": "icon-19.png",
    "default_popup": "options.html"
  }
}
